#include <iostream>
#include <stdio.h>

using namespace std;

int main()
{
    int t,n,m,x;
    
    scanf("%d",&t);
    
    while(t--)
    {
        
    scanf("%d %d %d",&n,&m,&x);
    
    if(n>=m)
    {
        if(n%m==0)
        {
            if(x==m)
               printf("YES\n");
            else 
               printf("NO\n");
        }       
        else if(n%m==x)
        {
               printf("YES\n");
            
        }
        else
               printf("NO\n");
    }
    
    else if(n==x) printf("YES\n");
    
    else printf("NO\n");


    }
    return 0;
}
